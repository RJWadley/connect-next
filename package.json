		"@cloudflare/workers-types": "^4.20241004.0",
		"@types/bun": "^1.1.10",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"postcss": "^8",
		"tailwindcss": "^3.4.13",
		"typescript": "^5",
		"vercel": "^37.6.1",
		"wrangler": "^3.80.0"
	}
